{
  "name": "svelte-meta-tags",
  "version": "2.8.0",
  "description": "Svelte Meta Tags is a plugin that makes managing your SEO easier in Svelte projects.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . --plugin prettier-plugin-svelte && eslint .",
    "format": "prettier --write . --plugin prettier-plugin-svelte",
    "test": "playwright test",
    "prepare": "husky install",
    "prepublishOnly": "pnpm package",
    "release": "changeset publish"
  },
  "dependencies": {
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.4",
    "@sveltejs/package": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-svelte": "^2.32.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "publint": "^0.2.0",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "peerDependencies": {
    "svelte": "^3.55.0 || ^4.0.0"
  },
  "exports": {
    "./JsonLd.svelte": {
      "types": "./dist/JsonLd.svelte.d.ts",
      "svelte": "./dist/JsonLd.svelte"
    },
    "./MetaTags.svelte": {
      "types": "./dist/MetaTags.svelte.d.ts",
      "svelte": "./dist/MetaTags.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
