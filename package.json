{
  "name": "svelte-meta-tags",
  "version": "2.8.0",
  "description": "Svelte Meta Tags is a plugin that makes managing your SEO easier in Svelte projects.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "playwright test",
    "prepare": "husky install",
    "prepublishOnly": "pnpm package",
    "release": "changeset publish"
  },
  "dependencies": {
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@playwright/test": "^1.35.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.31.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.1.12",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "svelte": "^3.55.0 || ^4.0.0"
  },
  "exports": {
    "./JsonLd.svelte": {
      "types": "./dist/JsonLd.svelte.d.ts",
      "svelte": "./dist/JsonLd.svelte"
    },
    "./MetaTags.svelte": {
      "types": "./dist/MetaTags.svelte.d.ts",
      "svelte": "./dist/MetaTags.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
