{
  "name": "svelte-meta-tags",
  "version": "2.6.1",
  "description": "Svelte Meta Tags is a plugin that makes managing your SEO easier in Svelte projects.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "package": "svelte-kit package",
    "prepare": "svelte-kit sync && husky install",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "pnpm cy:run",
    "release": "pnpm build && pnpm package && semantic-release"
  },
  "dependencies": {
    "schema-dts": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@sveltejs/adapter-auto": "1.0.0-next.50",
    "@sveltejs/kit": "1.0.0-next.350",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "cypress": "10.1.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.1",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "semantic-release": "19.0.3",
    "svelte": "3.48.0",
    "svelte-check": "2.7.2",
    "svelte-preprocess": "4.10.7",
    "svelte2tsx": "0.5.10",
    "tslib": "2.4.0",
    "typescript": "4.7.3"
  },
  "peerDependencies": {
    "svelte": "^3.39.0"
  }
}
