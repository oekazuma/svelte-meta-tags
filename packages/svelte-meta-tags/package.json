{
  "name": "svelte-meta-tags",
  "version": "3.1.1",
  "description": "Svelte Meta Tags provides components designed to help you manage SEO for Svelte projects",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prepublishOnly": "pnpm package && cp ../../README.md ./"
  },
  "dependencies": {
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.3",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "publint": "^0.2.7",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "peerDependencies": {
    "svelte": "^3.55.0 || ^4.0.0"
  },
  "exports": {
    "./JsonLd.svelte": {
      "types": "./dist/JsonLd.svelte.d.ts",
      "svelte": "./dist/JsonLd.svelte"
    },
    "./MetaTags.svelte": {
      "types": "./dist/MetaTags.svelte.d.ts",
      "svelte": "./dist/MetaTags.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
