{
  "name": "svelte-meta-tags",
  "version": "4.0.4",
  "description": "Svelte Meta Tags provides components designed to help you manage SEO for Svelte projects",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oekazuma/svelte-meta-tags.git",
    "directory": "packages/svelte-meta-tags"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:bench": "vitest bench",
    "prepublishOnly": "pnpm package && cp ../../README.md ./"
  },
  "dependencies": {
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.8.0",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "publint": "^0.2.12",
    "svelte": "^5.1.16",
    "svelte-check": "^4.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "exports": {
    "./JsonLd.svelte": {
      "types": "./dist/JsonLd.svelte.d.ts",
      "svelte": "./dist/JsonLd.svelte"
    },
    "./MetaTags.svelte": {
      "types": "./dist/MetaTags.svelte.d.ts",
      "svelte": "./dist/MetaTags.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
